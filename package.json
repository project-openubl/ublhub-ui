{
  "name": "xsender-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@data-driven-forms/pf4-component-mapper": "^3.6.1",
    "@data-driven-forms/react-form-renderer": "^3.6.1",
    "@konveyor/lib-ui": "^4.0.1",
    "@patternfly/react-code-editor": "4.2.76",
    "@patternfly/react-core": "4.128.2",
    "@patternfly/react-table": "4.27.24",
    "@react-keycloak/web": "^3.4.0",
    "@redhat-cloud-services/frontend-components-notifications": "^3.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "keycloak-js": "^13.0.0",
    "moment": "^2.29.1",
    "monaco-editor": "^0.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.3.2",
    "react-measure": "^2.5.2",
    "react-moment": "^1.1.1",
    "react-monaco-editor": "^0.43.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-use-websocket": "^2.6.1",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "xml-formatter": "^2.4.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "start": "react-app-rewired -r @cypress/instrument-cra start",
    "build": "react-app-rewired build",
    "build:instrumentation": "CYPRESS_INSTRUMENT_PRODUCTION=true react-app-rewired -r @cypress/instrument-cra build",
    "test": "react-app-rewired test",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "xsender:ui": "scripts/ui.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.5",
    "@cypress/instrument-cra": "^1.4.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/file-saver": "^2.0.2",
    "@types/js-yaml": "^4.0.1",
    "@types/react-measure": "^2.0.6",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "axios-mock-adapter": "^1.19.0",
    "cypress": "^7.2.0",
    "cypress-keycloak-commands": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "http-proxy-middleware": "^2.0.0",
    "husky": "^6.0.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^11.0.0",
    "monaco-editor-webpack-plugin": "^3.1.0",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "redux-devtools-extension": "^2.13.9",
    "source-map-explorer": "^2.5.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "nyc": {
    "exclude": "src/reportWebVitals.ts"
  }
}
